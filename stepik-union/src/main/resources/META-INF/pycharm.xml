<idea-plugin version="2">
    <extensions defaultExtensionNs="com.intellij">
        <treeStructureProvider implementation="org.stepik.plugin.projectView.pycharm.PyCharmTreeStructureProvider"/>
        <navbar implementation="org.stepik.plugin.projectView.pycharm.PyCharmNavBarModelExtension"/>
        <directoryProjectGenerator implementation="org.stepik.plugin.projectWizard.pycharm.StepikPyProjectGenerator"/>
        <refactoring.moveHandler implementation="org.stepik.plugin.refactoring.impl.pycharm.move.MoveHandlerDelegate" order="first"/>
        <renamePsiElementProcessor implementation="org.stepik.plugin.refactoring.impl.pycharm.rename.RenamePsiElementProcessor" order="first"/>
        <refactoring.safeDeleteProcessor implementation="org.stepik.plugin.refactoring.impl.pycharm.safeDelete.SafeDeleteProcessorDelegate" order="first"/>
    </extensions>
</idea-plugin>