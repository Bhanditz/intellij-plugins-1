<idea-plugin version="2">
    <extensions defaultExtensionNs="com.intellij">
        <treeStructureProvider implementation="org.stepik.core.projectView.pycharm.PyCharmTreeStructureProvider"/>
        <navbar implementation="org.stepik.core.projectView.pycharm.PyCharmNavBarModelExtension"/>
        <directoryProjectGenerator implementation="org.stepik.core.projectWizard.pycharm.StepikPyProjectGenerator"/>
        <refactoring.moveHandler implementation="org.stepik.plugin.refactoring.impl.pycharm.move.MoveHandlerDelegate"
                                 order="first"/>
        <renamePsiElementProcessor
                implementation="org.stepik.plugin.refactoring.impl.pycharm.rename.RenamePsiElementProcessor"
                order="first"/>
        <refactoring.safeDeleteProcessor
                implementation="org.stepik.plugin.refactoring.impl.pycharm.safeDelete.SafeDeleteProcessorDelegate"
                order="first"/>
    </extensions>
</idea-plugin>
